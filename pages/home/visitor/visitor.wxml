<!--pages/home/visitor/visitor.wxml-->
<import src="../../../common/foot.wxml" />
<view class="normal-container">
  <view class="mg20">
    <map
        style="width: 100%; height: 300px;"
        latitude="{{latitude}}"
        longitude="{{longitude}}"
        markers="{{markers}}"
        show-location="{{showLocation}}"
        enable-zoom=true
        >
      </map>
  </view>
  <view class="mg20 text-center">
    <i-button i-class="normal-btn wd40cen" bind:click="goToTiande" type="info" shape="circle" size="small">
      导   航
    </i-button>
  </view>
  <view class="mg20 text-center">
    <i-button i-class="normal-btn wd40cen" bind:click="searchCar" type="info" shape="circle" size="small">
      取车
    </i-button>
  </view>
  <view class="mg20 text-center">
    <i-button i-class="normal-btn wd40cen" bind:click="doScan" type="info" shape="circle" size="small">
      无牌车
    </i-button>
  </view>

</view>
