<!--pages/home/tenant/user.wxml-->
<import src="../../../common/foot.wxml" />
<view class="normal-container">
  <view class="mg20 text-center">
    <i-button i-class="normal-btn wd40cen" bind:click="apply" type="info" shape="circle" size="small">
      开卡申请
    </i-button>
    <i-button i-class="normal-btn wd40cen" bind:click="addCard" type="info" shape="circle" size="small">
      领卡
    </i-button>
  </view>
  <view class="mg20 text-center">
    <i-button i-class="normal-btn wd40cen" bind:click="payFee" type="info" shape="circle" size="small">
      缴费
    </i-button>
    <i-button i-class="normal-btn wd40cen" bind:click="payRecord" type="info" shape="circle" size="small">
      缴费记录
    </i-button>
    <i-button i-class="normal-btn wd40cen" bind:click="invoice" type="info" shape="circle" size="small">
      发票管理
    </i-button>
  </view>
  <view class="mg20 text-center">
    <i-button i-class="normal-btn wd40cen" bind:click="invite" type="info" shape="circle" size="small">
      邀请访客
    </i-button>
    <i-button i-class="normal-btn wd40cen" bind:click="myInvite" type="info" shape="circle" size="small">
      我邀请的访客
    </i-button>
  </view>
  <view class="mg20 text-center">
    <i-button i-class="normal-btn wd40cen" bind:click="searchCar" type="info" shape="circle" size="small">
      取车
    </i-button>
    <i-button i-class="normal-btn wd40cen" bind:click="doScan" type="info" shape="circle" size="small">
      无牌车
    </i-button>
    <i-button i-class="normal-btn wd40cen" bind:click="myCar" type="info" shape="circle" size="small">
      我的汽车
    </i-button>
    <i-button i-class="normal-btn wd40cen" bind:click="myCarSeat" type="info" shape="circle" size="small">
      我的车位
    </i-button>
  </view>

 <tabbar tabbar="{{tabbar}}"></tabbar> 
</view>

